{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "settings": {
        "inferno": {
          "createClass": "createClass",
          "pragma": "Inferno"
        },
        "propWrapperFunctions": [ "forbidExtraProps" ]
    },
    "extends": [
        "plugin:prettier/recommended",
        "plugin:inferno/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-console": 0,
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
    },
    "plugins": [
      "prettier",
      "inferno"
    ]
}